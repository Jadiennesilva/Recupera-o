#include <stdio.h>	
#include <stdlib.h>
#include <locale.h>


int main(){
	
	setlocale(LC_ALL, "portuguese_Brazil");

    int tabuada = 0;
    
    printf("Digite a tabuada que vocÃª deseja. \n");
    scanf("%d", &tabuada);

    for(int i = 1; i<=10; i++){
    printf("%d x %d = %d\n", i, tabuada, i * tabuada);
    }

	return 0; 
}
